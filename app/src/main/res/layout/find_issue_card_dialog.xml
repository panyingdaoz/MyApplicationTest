<androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:orientation="vertical"
    android:paddingTop="10dp"
    xmlns:android="http://schemas.android.com/apk/res/android">

        <TextView
            android:id="@+id/find_user_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="ID号"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/find_tv_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="3232654565"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@id/find_user_id"
            app:layout_constraintTop_toTopOf="@id/find_user_id" />

        <TextView
            android:id="@+id/find_attest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:text="认证"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/find_user_id" />

        <TextView
            android:id="@+id/find_tv_nickname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:text="昵称"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/find_attest" />

        <TextView
            android:id="@+id/find_nickname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:text="圣埃蒂安"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@id/find_tv_nickname"
            app:layout_constraintTop_toTopOf="@id/find_tv_nickname" />

        <TextView
            android:id="@+id/find_tv_gender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="性别"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/find_tv_nickname" />

        <TextView
            android:id="@+id/find_gender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:text="女"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@id/find_tv_gender"
            app:layout_constraintTop_toTopOf="@id/find_tv_gender" />

        <TextView
            android:id="@+id/find_tv_birthday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="120dp"
            android:layout_marginTop="16dp"
            android:text="生日"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@id/find_tv_nickname"
            app:layout_constraintTop_toBottomOf="@id/find_tv_nickname" />

        <TextView
            android:id="@+id/find_birthday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:text="2008-11-10"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@id/find_tv_birthday"
            app:layout_constraintTop_toTopOf="@id/find_tv_birthday" />

        <TextView
            android:id="@+id/find_tv_emotion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="情感状态"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/find_tv_gender" />

        <TextView
            android:id="@+id/find_emotion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="单身"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/find_gender"
            app:layout_constraintTop_toBottomOf="@id/find_gender" />

        <TextView
            android:id="@+id/find_tv_hometown"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16sp"
            android:text="家乡"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@id/find_tv_birthday"
            app:layout_constraintTop_toBottomOf="@id/find_tv_birthday" />

        <TextView
            android:id="@+id/find_hometown"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:text="北京"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@id/find_tv_hometown"
            app:layout_constraintTop_toTopOf="@id/find_tv_hometown" />

        <TextView
            android:id="@+id/find_tv_profession"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="职业"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/find_tv_emotion" />

        <TextView
            android:id="@+id/find_profession"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="学生"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/find_emotion"
            app:layout_constraintTop_toBottomOf="@id/find_emotion" />

        <TextView
            android:id="@+id/find_tv_school"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16sp"
            android:text="学校"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@id/find_tv_hometown"
            app:layout_constraintTop_toBottomOf="@id/find_tv_hometown" />

        <TextView
            android:id="@+id/find_school"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="清华大学"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/find_hometown"
            app:layout_constraintTop_toBottomOf="@id/find_hometown" />

        <TextView
            android:id="@+id/find_tv_hobby"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="兴趣爱好"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/find_tv_profession" />

        <TextView
            android:id="@+id/find_hobby"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="健身"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/find_profession"
            app:layout_constraintTop_toBottomOf="@id/find_profession" />

        <TextView
            android:id="@+id/find_tv_ideal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16sp"
            android:text="理想型"
            android:textColor="#3E4141"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@id/find_tv_school"
            app:layout_constraintTop_toBottomOf="@id/find_tv_school" />

        <TextView
            android:id="@+id/find_ideal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="1米9"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/find_school"
            app:layout_constraintTop_toBottomOf="@id/find_school" />

        <TextView
            android:id="@+id/tv_label"
            android:text="TAd的标签："
            android:textStyle="bold"
            android:layout_marginTop="24dp"
            android:textSize="16sp"
            android:textColor="#3E4141"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/find_tv_hobby"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <co.lujun.androidtagview.TagContainerLayout
            android:id="@+id/layout_label2"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_label"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:padding="10dp"
            android:background="#ffffff"
            app:container_enable_drag="false"
            android:visibility="gone"
            app:horizontal_interval="10dp"
            app:vertical_interval="10dp"
            app:tag_clickable="false"
            app:tag_theme="pure_teal" />

        <co.lujun.androidtagview.TagContainerLayout
            app:layout_constraintTop_toBottomOf="@+id/tv_label"
            android:id="@+id/layout_label"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            app:container_background_color="#00000000"
            app:container_border_color="#3300ff00"
            app:container_border_radius="0dp"
            app:container_border_width="4dp"
            app:container_enable_drag="true"
            app:horizontal_interval="10dp"
            app:tag_clickable="true"
            app:tag_theme="random"
            app:vertical_interval="10dp" />

        <TextView
            android:id="@+id/tv_signature"
            android:text="个性签名："
            android:textStyle="bold"
            android:layout_marginTop="24dp"
            android:textSize="16sp"
            android:textColor="#3E4141"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/layout_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/tv_signature_content"
            android:text="你走之后，我变得爱发呆，望着雨天，一边傻笑，一边流泪"
            android:textStyle="bold"
            android:layout_marginTop="8dp"
            android:textSize="16sp"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_signature"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

